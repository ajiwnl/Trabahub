<div class="maplayer">
	<div class="container pt-3">
		<div class="wrapper pb-5">
			<div class="row">
				<div class="col">
					<div class="header">
						<label class="mapheader">Interactive Search Map</label>
					</div>
				</div>
				<div class="col text-end pt-4" id="inputmap">
					<input type="text" id="textmap" class="ps-4 searchbox-map border-0 rounded-pill" placeholder="Where To?" onkeydown="handleKeyPress(event)" />
				</div>
				<div class="row pt-3">
					<div class="col">
						<div class="main">
							<iframe class="interactive-map border border-3 border-dark" scrolling="no" marginheight="0" marginwidth="0" id="map"
									src="https://maps.google.com/maps?width=100%25&amp;height=600&amp;hl=en&amp;q=+(cebu)&amp;t=&amp;z=14&amp;ie=UTF8&amp;iwloc=B&amp;output=embed">
								<a href="https://www.maps.ie/population/">Population mapping</a>
							</iframe>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>