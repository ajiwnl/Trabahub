@using Microsoft.AspNetCore.Http
@using Microsoft.AspNetCore.Mvc.Rendering


@{
    Layout = "_Layout";
    ViewBag.Username = Context.Session.GetString("Username");
    ViewBag.UserType = Context.Session.GetString("UserType");
    ViewBag.PriceOption = Context.Session.GetString("Option");
    ViewBag.PriceCount = Context.Session.GetInt32("Count");

    ViewBag.TotalListing = Context.Session.GetString("TotalListing");
    ViewBag.TotalUsers = Context.Session.GetString("TotalUsers");
    ViewBag.TotalBooks = Context.Session.GetString("TotalBooks");
    ViewBag.TotalCharges = Context.Session.GetString("TotalCharges");

    ViewBag.CurrentListing = Context.Session.GetString("CurrentListing");
}

